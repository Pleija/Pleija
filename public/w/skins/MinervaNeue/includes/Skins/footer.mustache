{{#data-footer}}
{{! `role` is implied in `footer` element, but added to mitigate a VoiceOver bug. }}
<footer class="mw-footer minerva-footer" role="contentinfo">
{{#data-minerva-history-link}}
  {{>history}}
{{/data-minerva-history-link}}
	<div class="post-content footer-content">
	{{{html-after-content}}}
	{{#data-logos}}<div class="minerva-footer-logo">{{>Logo}}<div class="github-fork">本站源码开源, 如有需要请访问: <a href="https://github.com/FiguWiki" target="_blank"><span>github.com/FiguWiki</span></a></div></div>{{/data-logos}}
	{{#data-info}}{{>footerItemList}}{{/data-info}}
	{{#data-places}}{{>footerItemList}}{{/data-places}}
	</div>
</footer>
{{/data-footer}}
